{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "cargo-check",
			"type": "shell",
			"command": "cargo",
			"args": [
				"check"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		},
		{
			"label": "cargo-test",
			"type": "shell",
			"command": "cargo",
			"args": [
				"test",
				"--all-features",
				"--quiet"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		}
	]
}